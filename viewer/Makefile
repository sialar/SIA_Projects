####### Compile

main.o: src/main.cpp src/glshaderwindow.h \
		src/openglwindow.h \
		../trimesh2/include/TriMesh.h \
		../trimesh2/include/Vec.h \
		../trimesh2/include/Box.h \
		../trimesh2/include/strutil.h \
		../trimesh2/include/Color.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o src/main.cpp

openglwindow.o: src/openglwindow.cpp src/openglwindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o openglwindow.o src/openglwindow.cpp

glshaderwindow.o: src/glshaderwindow.cpp src/glshaderwindow.h \
		src/openglwindow.h \
		../trimesh2/include/TriMesh.h \
		../trimesh2/include/Vec.h \
		../trimesh2/include/Box.h \
		../trimesh2/include/strutil.h \
		../trimesh2/include/Color.h \
		src/perlinNoise.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o glshaderwindow.o src/glshaderwindow.cpp

qrc_core-profile.o: qrc_core-profile.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o qrc_core-profile.o qrc_core-profile.cpp

moc_openglwindow.o: moc_openglwindow.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_openglwindow.o moc_openglwindow.cpp

moc_glshaderwindow.o: moc_glshaderwindow.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_glshaderwindow.o moc_glshaderwindow.cpp

